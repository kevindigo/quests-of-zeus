{
  "name": "oracle-of-delphi-pwa",
  "version": "0.1.0",
  "description": "A PWA implementation of the Oracle of Delphi boardgame with AI opponents",
  "tasks": {
    "dev": "deno run --watch --allow-net --allow-read main.ts",
    "start": "deno run --allow-net --allow-read main.ts",
    "build": "deno run -A build.ts",
    "test": "deno test --allow-read",
    "lint": "deno lint",
    "fmt": "deno fmt"
  },
  "exports": "./main.ts",
  "imports": {
    "@std/assert": "jsr:@std/assert@1"
  }
}